<div class="social-icons">
  {% if settings.facebook_url != blank %}
    <a href="{{ settings.facebook_url }}" target="_blank"><i class="fab fa-facebook-f"></i></a>
  {% endif %}
  
  {% if settings.instagram_url != blank %}
    <a href="{{ settings.instagram_url }}" target="_blank"><i class="fab fa-instagram"></i></a>
  {% endif %}
  
  {% if settings.twitter_url != blank %}
    <a href="{{ settings.twitter_url }}" target="_blank"><i class="fab fa-twitter"></i></a>
  {% endif %}
  
  {% if settings.linkedin_url != blank %}
    <a href="{{ settings.linkedin_url }}" target="_blank"><i class="fab fa-linkedin-in"></i></a>
  {% endif %}
  
  {% if settings.youtube_url != blank %}
    <a href="{{ settings.youtube_url }}" target="_blank"><i class="fab fa-youtube"></i></a>
  {% endif %}
  
  {% if settings.tiktok_url != blank %}
    <a href="{{ settings.tiktok_url }}" target="_blank"><i class="fab fa-tiktok"></i></a>
  {% endif %}
  
  {% if settings.pinterest_url != blank %}
    <a href="{{ settings.pinterest_url }}" target="_blank"><i class="fab fa-pinterest-p"></i></a>
  {% endif %}
  
  {% if settings.snapchat_url != blank %}
    <a href="{{ settings.snapchat_url }}" target="_blank"><i class="fab fa-snapchat-ghost"></i></a>
  {% endif %}
  
</div>
